{
    "manifest_version": 2,
    "name": "MSDN Column Resize",
    "description":"Adds a Separator bar between columns on MSDN Pags that can be clicked to resize columns",
    "version": "0.5",
    "content_scripts": [
        {
            "matches": [
                "https://docs.microsoft.com/*",
                "http://docs.microsoft.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "icons": {
        "16": "MSDNFix16.png",
        "32": "MSDNFix32.png",
        "48": "MSDNFix48.png",
        "128": "MSDNFix128.png"
    }
}